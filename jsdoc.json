{
  "opts": {
    "template": "node_modules/jsdoc-template",
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  },
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["modules"],
    "excludePattern": ".+\\.spec\\.js$"
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "referenceTitle": "Smart-Modules",
    "disableSort": true
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "excludeTags": ["author", "license"]
  }
}
